{
  "name": "fia-webhook",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: aws-hackathon",
  "customName": false,
  "customRole": false,
  "handler": "fia-webhook/handler.handler",
  "timeout": 30,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "optimize": true,
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "x-api-key",
        "X-Amz-Security-Token"
      ]
    }
  },
  "endpoints": [
    {
      "path": "api/fia-webhook",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": true,
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplate}",
      "responses": {
        "400": {
          "selectionPattern": ".*\"status\":400.*",
          "statusCode": "400",
          "responseTemplates": {
            "application/json": "$input.path('$.errorMessage')"
          }
        },
        "401": {
          "selectionPattern": "^\\[Unauthorized\\].*",
          "statusCode": "401"
        },
        "403": {
          "selectionPattern": "^\\[Forbidden\\].*",
          "statusCode": "403"
        },
        "404": {
          "selectionPattern": ".*\"status\":404.*",
          "statusCode": "404",
          "responseTemplates": {
            "application/json": "$input.path('$.errorMessage')"
          }
        },
        "422": {
          "selectionPattern": ".*\"status\":422.*",
          "statusCode": "422",
          "responseTemplates": {
            "application/json": "$input.path('$.errorMessage')"
          }
        },
        "500": {
          "selectionPattern": "^\\[ServerError\\].*",
          "statusCode": "500",
          "responseTemplates": {
            "application/json": "{\"error\": \"$input.path('$.errorMessage')\"}"
          }
        },
        "504": {
          "selectionPattern": "^\\[Task timed out\\].*",
          "statusCode": "504"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "DB_NAME": "${DB_NAME}",
    "DB_USER": "${DB_USER}",
    "DB_PASSWORD": "${DB_PASSWORD}",
    "DB_HOST": "${DB_HOST}",
    "DB_PORT": "${DB_PORT}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}